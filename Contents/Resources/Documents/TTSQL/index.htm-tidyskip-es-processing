<dd class="l2ix">SELECT object privilege, definition, <a href="privileges.htm#sthref1936">7.2</a></dd>
<dd class="l2ix">SELECT object privilege, hierarchy, <a href="privileges.htm#sthref2001">7.3</a></dd>
</dl>
</dd>
<dd class="l1ix">views--also see materialized views</dd>
</dl>
<a id="W"></a>
<hr />
<h2>W</h2>
<dl class="l1ix">
<dd class="l1ix">WHEN reserved word, <a href="reserved.htm#sthref2109">8</a></dd>
<dd class="l1ix">WHERE clause, SELECT statement, <a href="state.htm#sthref1695">6.2.6</a></dd>
<dd class="l1ix">WHERE reserved word, <a href="reserved.htm#sthref2110">8</a></dd>
<dd class="l1ix">WITH clause, SELECT statement, <a href="state.htm#sthref1710">6.2.6</a></dd>
<dd class="l1ix">WITH reserved word, <a href="reserved.htm#sthref2111">8</a></dd>
</dl>
<a id="X"></a>
<hr />
<h2>X</h2>
<dl class="l1ix">
<dd class="l1ix">XLA system privilege, <a href="privileges.htm#sthref1906">7.1</a></dd>
</dl>
